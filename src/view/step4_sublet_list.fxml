<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="controller.Step4SubletController"
      spacing="20" alignment="CENTER" stylesheets="@login.css">

    <Label text="Available Sublets" />
    
    <TableView fx:id="tableView" prefWidth="600">
        <columns>
            <TableColumn fx:id="titleColumn" text="Title" prefWidth="150"/>
            <TableColumn fx:id="locationColumn" text="Location" prefWidth="150"/>
            <TableColumn fx:id="priceColumn" text="Price" prefWidth="100"/>
            <TableColumn fx:id="descriptionColumn" text="Description" prefWidth="200"/>
        </columns>
    </TableView>
    
    <Button text="Back" onAction="#handleBack" />
</VBox>

